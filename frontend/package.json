{
  "name": "unifi-network-map-card",
  "version": "0.3.5",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "esbuild src/unifi-network-map.ts --bundle --format=esm --target=es2020 --define:__CARD_VERSION__=\\\"$npm_package_version\\\" --outfile=dist/unifi-network-map.js",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "devDependencies": {
    "@types/jest": "30.0.0",
    "@typescript-eslint/eslint-plugin": "8.56.1",
    "@typescript-eslint/parser": "8.56.1",
    "esbuild": "0.27.3",
    "eslint": "10.0.2",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jest-util": "^30.2.0",
    "prettier": "3.8.1",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "dompurify": "^3.3.1"
  }
}
